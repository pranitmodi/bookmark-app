{
  "manifest_version": 3,
  "name": "Bookmark AI Assistant",
  "version": "1.0.0",
  "description": "An AI-powered Chrome extension that intelligently organizes your bookmarks based on your existing folder structure.",
  "action": {
    "default_popup": "index.html",
    "default_title": "Bookmark AI Assistant"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  }
}